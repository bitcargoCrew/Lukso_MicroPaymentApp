{
  "name": "backend_up_app",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "dotenv -- ts-node-dev --respawn --exit-child --quiet app/index.ts",
    "build": "rm -rf build; yarn run check && tsup app/index.ts --clean --sourcemap --format cjs --out-dir build",
    "check": "tsc --noEmit",
    "serve": "dotenv -- node build/index.js",
    "start-db": "docker-compose up -d",
    "deploy-db": "dotenv -c -- prisma migrate deploy"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv-cli": "^7.4.1",
    "ethers": "^6.11.1",
    "express": "^4.19.2",
    "firebase-admin": "^12.1.1",
    "node-fetch": "^3.3.2",
    "thirdweb": "^5.3.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/uuid": "^9.0.8"
  }
}
